-- actors! (thing that move)
function actor (config)
 -- default props
	local star={
		 k= 0, -- sprite 0 index
	  x= 0, -- x position
	  y= 0, -- y position
	  dx= 0, -- delta x per tick
	  dy= 0, -- delta y per tick
	  dir= 1, -- direction x-axis
	  has_gravity= true,
	  is_visible= true,
	  hit_floor= false,
	  hit_wall= false,
	  hit_ceil= false,
	  can_open_door= false,
	}
	-- mix in the config
	for k, v in pairs(config) do
		star[k] = v
	end
	
	-- add to the actor list
	add(actors, star)
	
	return star
end


function door(config)
 local star = {
 	start_x=0,
 	end_x=0,
		next_level=1,
 	hit_by=nil,
 	hit_dir="",
 	is_open=false,
 }
 
 -- mix in the config
	for k, v in pairs(config) do
		star[k] = v
	end
	
 add(doors, star)
 return star
end
